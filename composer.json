{
  "name": "rejoice/rejoice-framework",
  "type": "library",
  "description": "Write easily your USSD based applications.",
  "keywords": [
    "ussd",
    "whatsapp"
  ],
  "homepage": "https://github.com/rejoice-framework/framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Prince Damien Dorcis-Akpaglo",
      "email": "princedorcis@gmail.com",
      "role": "Maintainer",
      "homepage": "https://github.com/Prinx"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.3",
    "prinx/dotenv": "dev-master",
    "prinx/notify": "dev-master",
    "prinx/phputils": "dev-master",
    "prinx/dir-dot-config": "dev-master",
    "prinx/string-utils": "dev-master",
    "prinx/array-utils": "dev-master",
    "prinx/os-utils": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "9.2.5"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Prinx\\Rejoice\\": "src/"
    }
  }
}